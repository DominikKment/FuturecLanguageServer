	"New Script": {
		"prefix": "Create Script",
		"body": [
			"CHANGE:\t$CURRENT_DATE.$CURRENT_MONTH.$CURRENT_YEAR\t${4:Name}\t$LINE_COMMENT AP-ID:${5:ID}",
			"\t\t${6:Erstellt}",
			"",
			"///////////////////////////////////////////////////////////////////////////////",
			"SCRIPT:${1:Scriptnumber},${2:Scriptname},0,0,0,${3:Scripticon}",
			"",
			"\tH.LockThisScript();",
			"\tS.RequestSaveAndUnlock(m_TabNr, m_RecNr);",
			"\tS.LockActualRecord();",
			"",
			"\t$0",
			"",
			"\tS.UpdateActualRecord();",
			"\tS.UnlockActualRecord();",
			"",
			"ENDSCRIPT",
			"///////////////////////////////////////////////////////////////////////////////"
		],
		"description":"Erstellt ein neues Skript mit Changekommentar usw."
	},
	"New Function": {
		"prefix": "Create Function",
		"body": [
			"$LINE_COMMENT ${5:Documentation}",
			"FUNCTION: ${1|void,CString,CTable,int,double|} ${2:Functionname}(${3:Parameter});",
			"\t$1 ${4:Variabale};",
			"\t$0",
			"\t",
			"\tfuncreturn ${4:Variabale};",
			"ENDFUNCTION;"
		],
		"description":"Erstellt eine neue Function"
	},
	"New Changecomment": {
